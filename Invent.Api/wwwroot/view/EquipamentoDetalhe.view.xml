<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="ui5.gestaoequipamento.controller.EquipamentoCadastro"
>
    <Page
        title="{i18n>tituloPaginaCadastro}"
        showNavButton="true"
        navButtonPress=".aoClicarEmVoltar"
    >
        <headerContent>
            <Button
                type="Emphasized"
                text="{i18n>botaoEditar}"
                icon="sap-icon://edit"
                press=".aoClicarEmEditar"
            />
        </headerContent>
    </Page>
</mvc:View>
